{
  "name": "smart-atc-server",
  "version": "0.1.0",
  "description": "Smart ATC Backend MVP - Fastify API for ATC training sessions",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:vercel": "vercel dev --yes",
    "build": "prisma generate && tsc && cp -r src/prompts dist/prompts && mkdir -p dist/common && cp src/common/fsm.yaml dist/common/fsm.yaml",
    "start": "NODE_ENV=production node dist/server.js",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "test": "dotenv -e .env.test -- vitest",
    "test:once": "dotenv -e .env.test -- vitest run",
    "test:atc-agent": "dotenv -e .env.test -- vitest run --reporter=verbose src/__tests__/atc-agent-ground.test.ts src/__tests__/atc-agent-departure.test.ts src/__tests__/atc-agent-arrival.test.ts",
    "test:atc-agent:watch": "dotenv -e .env.test -- vitest --reporter=verbose src/__tests__/atc-agent-*.test.ts",
    "test:atc-agent:ground": "dotenv -e .env.test -- vitest run --reporter=verbose src/__tests__/atc-agent-ground.test.ts",
    "test:atc-agent:departure": "dotenv -e .env.test -- vitest run --reporter=verbose src/__tests__/atc-agent-departure.test.ts",
    "test:atc-agent:arrival": "dotenv -e .env.test -- vitest run --reporter=verbose src/__tests__/atc-agent-arrival.test.ts",
    "test:evaluation": "dotenv -e .env.test -- vitest run --reporter=verbose src/__tests__/evaluation-agent-regression.test.ts",
    "test:evaluation:watch": "dotenv -e .env.test -- vitest --reporter=verbose src/__tests__/evaluation-agent-regression.test.ts",
    "test:state-transition": "dotenv -e .env.test -- vitest run --reporter=verbose src/__tests__/state-transition-agent-regression.test.ts",
    "test:state-transition:watch": "dotenv -e .env.test -- vitest --reporter=verbose src/__tests__/state-transition-agent-regression.test.ts",
    "test:tts": "dotenv -e .env.test -- tsx scripts/test_tts.js",
    "test:openai": "dotenv -e .env.test -- tsx scripts/test-openai-gpt4.1.ts",
    "test:s3": "dotenv -e .env.development -- tsx scripts/test-s3-upload.ts",
    "test:audio:simple": "tsx scripts/test-audio-noise-simple.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production -- prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:deploy:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "prisma:deploy:dev": "dotenv -e .env.development -- prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "prisma:studio:dev": "dotenv -e .env.development -- prisma studio",
    "prisma:studio:prod": "dotenv -e .env.production -- prisma studio",
    "prisma:migrate:stats:dev": "dotenv -e .env.development -- prisma migrate status",
    "prisma:migrate:stats:prod": "dotenv -e .env.production -- prisma migrate status",
    "prisma:create:dev": "dotenv -e .env.development -- prisma migrate dev --create-only",
    "prisma:create:prod": "dotenv -e .env.production -- prisma migrate dev --create-only",
    "sync:airports": "dotenv -e .env.development -- tsx scripts/sync_airports.ts",
    "sync:airports:prod": "dotenv -e .env.production -- tsx scripts/sync_airports.ts",
    "export:data": "dotenv -e .env.development -- tsx scripts/export_data.ts",
    "export:data:prod": "dotenv -e .env.production -- tsx scripts/export_data.ts",
    "fetch:liveatc": "dotenv -e .env.development -- tsx scripts/fetch_liveatc_feeds.ts",
    "fetch:liveatc:prod": "dotenv -e .env.production -- tsx scripts/fetch_liveatc_feeds.ts",
    "update:freqs": "dotenv -e .env.development -- tsx scripts/update_airport_freqs.ts",
    "update:freqs:prod": "dotenv -e .env.production -- tsx scripts/update_airport_freqs.ts",
    "tier-limits:view": "dotenv -e .env.development -- tsx scripts/view-tier-limits.ts",
    "tier-limits:view:prod": "dotenv -e .env.production -- tsx scripts/view-tier-limits.ts",
    "tier-limits:update": "dotenv -e .env.development -- tsx scripts/update-tier-limits.ts",
    "tier-limits:update:prod": "dotenv -e .env.production -- tsx scripts/update-tier-limits.ts",
    "fsm:visualize:data": "tsx tools/generate-fsm-visualizer-data.ts",
    "fsm:visualize": "pnpm run fsm:visualize:data && echo '\\n############################################################' && echo '#  OPEN http://localhost:8888/tools/fsm-visualizer.html  #' && echo '############################################################\\n' && npx serve -p 8888 ."
  },
  "keywords": [
    "atc",
    "aviation",
    "training",
    "fastify",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apple/app-store-server-library": "^2.0.0",
    "@aws-sdk/client-s3": "^3.937.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/websocket": "^11.2.0",
    "@prisma/client": "5.22.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "bcryptjs": "^3.0.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^11.0.0",
    "dotenv-flow": "^4.1.0",
    "expo-server-sdk": "^4.0.0",
    "fastify": "^5.1.0",
    "fastify-type-provider-zod": "^4.0.2",
    "fish-audio": "^0.0.3",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "ngrok": "5.0.0-beta.2",
    "openai": "^6.6.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "prisma": "5.22.0",
    "undici": "^6.22.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.9.0",
    "@types/node-fetch": "^2.6.13",
    "@vercel/node": "^5.5.28",
    "https-proxy-agent": "^7.0.6",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
