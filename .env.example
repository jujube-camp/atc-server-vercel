# Database (Neon pooled - copy your connection string from Neon Console, do not commit real credentials)
# Format: postgresql://USER:PASSWORD@ep-xxx-pooler.region.aws.neon.tech/neondb?sslmode=require
DATABASE_URL="postgresql://USER:PASSWORD@ep-xxx-pooler.region.aws.neon.tech/neondb?sslmode=require"

# LLM
OPENAI_API_KEY=sk-your-openai-api-key-here

# JWT
JWT_SECRET="your-jwt-secret-at-least-32-chars"

# Server
PORT=3000
NODE_ENV=development

# Optional: Log Level (trace, debug, info, warn, error, fatal)
LOG_LEVEL=info

HTTPS_PROXY=http://127.0.0.1:7897

APPLE_CLIENT_ID=com.jujubecamp.aviateai

FISH_AUDIO_API_KEY=your-fish-audio-api-key
# Voice 1 = ATC prompt; Voice 2 = sample response (pilot)
FISH_AUDIO_REFERENCE_ID=your-fish-audio-reference-id-voice-1
FISH_AUDIO_REFERENCE_ID_VOICE_2=your-fish-audio-reference-id-voice-2

# Vercel Blob (for flashcard TTS audio; required for seed-flashcards script)
BLOB_READ_WRITE_TOKEN=your-vercel-blob-token

AWS_REGION=us-west-2
AWS_S3_AUDIO_BUCKET=aviate-ai-public
AWS_S3_AUDIO_PREFIX=cockpit/audio
AWS_ACCESS_KEY_ID=xxxx
AWS_SECRET_ACCESS_KEY=xxxx